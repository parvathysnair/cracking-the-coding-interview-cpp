#include<iostream>
#include<vector>

using namespace std;


class graph{

  int v;
  vector<int> adj[];
  public:
  void addedge(int a,int b){
    adj[a].push_back(b);
  }

};


int main(){
  int v=6;
  graph g1;
  g1.addedge(1,2);
  g1.addedge(1,3);
  g1.addedge(2,6);
  g1.addedge(3,4);
  g1.addedge(4,5);
  g1.addedge(4,6);
  g1.addedge(6,5);





  return 1;
}
